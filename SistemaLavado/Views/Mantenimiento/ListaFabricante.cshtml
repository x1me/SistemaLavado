@model  List<SistemaLavado.Models.pa_RetornaFabricante_Result>
@{
    ViewBag.Title = "ListaFabricante";
}

<h2>Lista De Los Fabricantes</h2>
<br />
<p>Esta lista abarca todos los paises con sus respectivos códigos</p>
<br />  
<a href="~/Mantenimiento/InsertarFabricante">Nuevo Fabricante</a>
<br />
<table class="table">
    <tr>
        <th>Código </th>
        <th>Pais</th>

    </tr>
    @{
        foreach (SistemaLavado.Models.pa_RetornaFabricante_Result PosicionModelo in this.Model)
        {
            <tr>
                <td>@PosicionModelo.codigo</td>
                <td>@PosicionModelo.pais</td>
            </tr>
        }
    }
</table>
